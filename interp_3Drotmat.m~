function Ro = interp3Drotmat(xi, Ri, xo, method)
%% arri = interp3Darray(arr, x, xi) 
%
% Interpolates a 3x3xn array of 3x3 rotation matrices from xi to xo
% 
% INPUTS:
%   xi:
%   Ri:
%   xo:
%   method:
% 
% OUTPUTS:
%   Ro:
%
%%
len = length(xi);
Ro = zeros(3,3,len);
ei = zeros(3,len); % euler angles for input array
eo = ei; % euler angles for output array
for n = length(xi) % determine input euler angles
  ei(:,n) = solve_rotate3(Ri(:,:,n));
end
eo = interp1(xi,ei(1,:),




end